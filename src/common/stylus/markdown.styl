markdown()
	ul
		list-style: disc inside
	ol
		list-style: decimal inside
	ul li, ol li
		font-size: 16px
		line-height: 1.3
	img
		max-width: 100%
	strong
		font-weight: 700
	h1,h2,h3,h4,h5,h6,p,blockquote 
		margin: 0
		padding: 0
	table 
		margin: 10px 0 15px 0
		font-size: 16px
		border-collapse: collapse
	td,th 
		border: 1px solid #ddd;
		padding: 3px 10px
	th
		padding: 5px 10px
	a
		color: #0069d6
	a:hover 
		color: #0050a3
		text-decoration: none;
	a img
		border: none
	p
		margin-bottom: 9px
		font-size: 16px
	h1,h2,h3,h4,h5,h6
		color: #404040
		line-height: 36px
	h1
		margin-bottom: 18px
		font-size: 30px
	h2
		font-size: 24px
	h3
		font-size: 18px
	h4
		font-size: 16px
	h5
		font-size: 14px
	h6
		font-size: 13px
	hr
		margin: 0 0 19px
		border: 0
		border-bottom: 1px solid #ccc
	blockquote
		padding: 13px 13px 21px 15px
		margin: 18px 0
		border-left: 5px solid #eee
	blockquote p
		font-size: 16px
		font-weight: 300
		line-height: 18px
		margin-bottom: 0
	code,pre
		font-family: Monaco, Andale Mono, Courier New, monospace
	code
		overflow-y: auto
		background-color: #fee9cc
		color: rgba(0, 0, 0, 0.75)
		padding: 1px 3px
		font-size: 14px
		-webkit-border-radius: 3px
		-moz-border-radius: 3px
		border-radius: 3px
	pre
		display: block
		padding: 14px
		margin: 0 0 18px
		line-height: 16px
		font-size: 14px
		border: 1px solid #d9d9d9
		white-space: pre-wrap
		word-wrap: break-word
		background-color: #1E1E1E
	pre code
		color:#737373
		font-size: 14px
		padding: 0
		background-color: #1E1E1E
	sup
		font-size: 0.83em;
		vertical-align: super;
		line-height: 0
	*
		-webkit-print-color-adjust: exact